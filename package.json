{
  "name": "auto-scaffold",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.28.0",
  "description": "",
  "license": "MIT",
  "homepage": "https://github.com/posva/auto-scaffold#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/auto-scaffold.git"
  },
  "bugs": {
    "url": "https://github.com/posva/auto-scaffold/issues"
  },
  "keywords": [
    "unplugin",
    "vite",
    "scaffold",
    "template",
    "dev-tools"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./astro": "./dist/astro.js",
    "./esbuild": "./dist/esbuild.js",
    "./farm": "./dist/farm.js",
    "./nuxt": "./dist/nuxt.js",
    "./rollup": "./dist/rollup.js",
    "./rspack": "./dist/rspack.js",
    "./types": "./dist/types.js",
    "./vite": "./dist/vite.js",
    "./webpack": "./dist/webpack.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown -w",
    "lint": "oxlint",
    "format": "oxfmt . .github",
    "format:check": "oxfmt --check .",
    "play": "npm -C playground run dev",
    "prepublishOnly": "npm run build",
    "start": "node src/index.ts",
    "test": "vitest"
  },
  "peerDependencies": {
    "@farmfe/core": ">=1",
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "esbuild": "*",
    "rollup": "^3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@farmfe/core": {
      "optional": true
    },
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "pathe": "^2.0.3",
    "unplugin": "^2.3.11"
  },
  "devDependencies": {
    "@nuxt/kit": "^4.2.2",
    "@nuxt/schema": "^4.2.2",
    "@posva/prompts": "^2.4.4",
    "@types/node": "^25.0.8",
    "@types/semver": "^7.7.1",
    "chalk": "^5.6.2",
    "execa": "^9.6.1",
    "lint-staged": "^16.2.7",
    "nodemon": "^3.1.10",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.39.0",
    "rollup": "^4.41.0",
    "semver": "^7.7.3",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "0.20.0-beta.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17",
    "webpack": "^5.99.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,mjs,json,cjs,md,ts,yml,tsx,jsx}": [
      "oxfmt",
      "oxlint --fix"
    ]
  }
}
